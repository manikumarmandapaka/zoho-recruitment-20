public class Pattern
{
    public static void main(String[] args)
    {
        int star=0;
        int space=5;
        int n=1;
        for(int i=1;i<=5;i++)
        {
            star++;
            space--;
            for(int j=1;j<=space;j++)
            {
              System.out.Print(' ');
            }
            for(int k=1;k<=star;k++)
            {
              System.out.Print(n);
              n++;
            }
            System.out.Println();
        }
    }
}
